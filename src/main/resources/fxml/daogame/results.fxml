<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="daogame.controller.ResultsController">
    <children>
        <Label text="All time top 5">
            <font>
                <Font size="30.0" />
            </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
        </Label>
        <TableView fx:id="resultTable2" prefHeight="145.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="player1Column2" prefWidth="135.0" text="Player 1" />
                <TableColumn fx:id="player2Column2" minWidth="1.5999755859375" prefWidth="135.0" text="Player 2" />
                <TableColumn fx:id="winnerColumn2" prefWidth="135.0" text="Winner" />
                <TableColumn fx:id="durationColumn2" prefWidth="153.5999755859375" text="Duration" />
                <TableColumn fx:id="dateColumn2" prefWidth="75.0" text="Date" />
            </columns>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
        </TableView>
        <Label fx:id="winnerLabel" text="The winner is ">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button mnemonicParsing="false" onAction="#handleDeleteButtonClicked" text="Delete" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </HBox>
        <TableView fx:id="resultTable" prefHeight="200.0">
            <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY" /></columnResizePolicy>
            <columns>
                <TableColumn fx:id="player1Column" prefWidth="135.0" text="Player 1" />
                <TableColumn fx:id="player2Column" minWidth="1.5999755859375" prefWidth="135.0" text="Player 2" />
                <TableColumn fx:id="winnerColumn" prefWidth="135.0" text="Winner" />
                <TableColumn fx:id="durationColumn" prefWidth="153.5999755859375" text="Duration" />
                <TableColumn fx:id="dateColumn" prefWidth="75.0" text="Date" />
            </columns>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
        </TableView>
        <HBox alignment="CENTER" prefWidth="200.0" spacing="40.0">
            <children>
                <Button fx:id="rematchButton" mnemonicParsing="false" onMouseClicked="#onRematchButtonClicked" style="-fx-base: greenyellow;" text="Rematch" />
                <Button fx:id="homeButton" mnemonicParsing="false" onMouseClicked="#onHomeButtonClicked" style="-fx-base: lightblue;" text="Jump to Home Screen" />
                <Button fx:id="exitButton" mnemonicParsing="false" onMouseClicked="#onExitButtonClicked" style="-fx-base: tomato;" text="Exit" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets left="20.0" right="20.0" />
    </padding>
</VBox>
